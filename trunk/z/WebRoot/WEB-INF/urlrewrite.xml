<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.1.dtd">
<urlrewrite>

	<rule>
		<from>^/$</from>
		<to last ="true">/index.d</to>
	</rule>
	
	<rule>
		<from>^/search?(.*)$</from>
		<to last ="true">/s.d?</to>
	</rule>




</urlrewrite>
